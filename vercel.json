{
  "version": 2,
  "builds": [
    {
      "src": "app/package.json",
      "use": "@vercel/next@latest",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ],
  "build": {
    "env": {
      "NPM_CONFIG_PACKAGE_LOCK": "false",
      "YARN_PRODUCTION": "false"
    }
  }
}